{
  "name": "clusterization",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44439 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44439 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build",
    "build:ssr": "ng run Clusterization:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^17.1.3",
    "@angular/animations": "^17.1.3",
    "@angular/cdk": "^17.1.2",
    "@angular/cli": "^17.1.3",
    "@angular/compiler": "^17.1.3",
    "@angular/compiler-cli": "^17.1.3",
    "@angular/core": "^17.1.3",
    "@angular/forms": "^17.1.3",
    "@angular/localize": "^17.2.1",
    "@angular/material": "^17.1.2",
    "@angular/platform-browser": "^17.1.3",
    "@angular/platform-browser-dynamic": "^17.1.3",
    "@angular/platform-server": "^17.1.3",
    "@angular/router": "^17.1.3",
    "@auth0/angular-jwt": "^5.2.0",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^20.11.17",
    "bootstrap": "^5.3.2",
    "date-fns": "^3.6.0",
    "jasmine-core": "^5.1.2",
    "jquery": "^3.7.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ngx-popperjs": "^17.0.0",
    "ngx-toastr": "^18.0.0",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.1",
    "run-script-os": "^1.1.6",
    "rxjs": "^7.8.1",
    "zone.js": "^0.14.3"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
